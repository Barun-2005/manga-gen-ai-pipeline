{
  "1": {
    "inputs": {
      "image": "assets/styles/{style_name}.png",
      "upload": "image"
    },
    "class_type": "LoadImage",
    "_meta": {
      "title": "Load Style Reference"
    }
  },
  "2": {
    "inputs": {
      "image": [
        "1",
        0
      ],
      "resolution": 512
    },
    "class_type": "ImageScale",
    "_meta": {
      "title": "Resize Style Image"
    }
  },
  "3": {
    "inputs": {
      "image": [
        "2",
        0
      ]
    },
    "class_type": "CannyEdgePreprocessor",
    "_meta": {
      "title": "Extract Line Art"
    }
  },
  "4": {
    "inputs": {
      "filename_prefix": "style_processed_{style_name}",
      "images": [
        "3",
        0
      ]
    },
    "class_type": "SaveImage",
    "_meta": {
      "title": "Save Processed Style"
    }
  },
  "5": {
    "inputs": {
      "filename_prefix": "style_original_{style_name}",
      "images": [
        "2",
        0
      ]
    },
    "class_type": "SaveImage",
    "_meta": {
      "title": "Save Original Style"
    }
  }
}
